<?xml version="1.0" encoding="utf-8"?>
<manifest
    package="com.danil.asus.lab3"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".StartActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/main_label">
        </activity>
        <activity
            android:name=".MeetingInfoActivity"
            android:label="@string/title_activity_meeting_info">
        </activity>
        <activity
            android:name=".NewMeetingActivity"
            android:label="@string/title_activity_new_meeting">
        </activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET"/>

</manifest>
